{
    "type": "object",
    "properties": {
        "APPLICATION_NAME": {
            "oneOf": [
                {
                    "type": "string"
                },
                {
                    "type": "null"
                }
            ]
        },
        "APP_URL": {
            "oneOf": [
                {
                    "oneOf": [
                        {
                            "type": "string",
                            "format": "uri"
                        },
                        {
                            "enum": ["http://localhost/"]
                        }
                    ]
                },
                {
                    "type": "null"
                }
            ]
        },
        "API_PORT": {
            "oneOf": [
                {
                    "type": "number"
                },
                {
                    "type": "null"
                }
            ]
        },
        "API_BASE": {
            "oneOf": [
                {
                    "type": "string"
                },
                {
                    "type": "null"
                }
            ]
        },
        "LOG_TIMESTAMP": {
            "oneOf": [
                {
                    "type": "boolean"
                },
                {
                    "type": "null"
                }
            ]
        },
        "LOG_LEVEL": {
            "oneOf": [
                {
                    "enum": ["trace", "debug", "info", "warn", "error"]
                },
                {
                    "type": "null"
                }
            ]
        },
        "ADMIN_EMAIL": {
            "oneOf": [
                {
                    "type": "string",
                    "format": "email"
                },
                {
                    "type": "null"
                }
            ]
        },
        "FEDERATION": {
            "oneOf": [
                {
                    "type": "boolean"
                },
                {
                    "type": "null"
                }
            ]
        },
        "AUTH_PROVIDERS": {
            "oneOf": [
                {
                    "minItems": 1,
                    "maxItems": 18,
                    "type": "array",
                    "items": {
                        "oneOf": [
                            {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "oneOf": [
                                            {
                                                "enum": ["password"]
                                            },
                                            {
                                                "enum": ["code"]
                                            }
                                        ]
                                    }
                                },
                                "required": ["name"]
                            },
                            {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "oneOf": [
                                            {
                                                "enum": ["github"]
                                            }
                                        ]
                                    },
                                    "clientID": {
                                        "type": "string"
                                    },
                                    "clientSecret": {
                                        "type": "string"
                                    },
                                    "pkce": {
                                        "oneOf": [
                                            {
                                                "type": "boolean"
                                            },
                                            {
                                                "type": "null"
                                            }
                                        ]
                                    },
                                    "query": {
                                        "oneOf": [
                                            {
                                                "type": "object",
                                                "propertyNames": {
                                                    "type": "string"
                                                },
                                                "additionalProperties": {
                                                    "type": "string"
                                                }
                                            },
                                            {
                                                "type": "null"
                                            }
                                        ]
                                    },
                                    "trusted": {
                                        "oneOf": [
                                            {
                                                "type": "boolean"
                                            },
                                            {
                                                "type": "null"
                                            }
                                        ],
                                        "default": false
                                    }
                                },
                                "required": [
                                    "name",
                                    "clientID",
                                    "clientSecret",
                                    "trusted"
                                ]
                            }
                        ]
                    }
                },
                {
                    "type": "null"
                }
            ]
        },
        "FEDERATION_TARGETS": {
            "oneOf": [
                {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "name": {
                                "type": "string"
                            },
                            "url": {
                                "type": "string"
                            }
                        },
                        "required": ["name", "url"]
                    }
                },
                {
                    "type": "null"
                }
            ]
        },
        "DB_CONFIG": {
            "oneOf": [
                {
                    "type": "object",
                    "properties": {
                        "host": {
                            "oneOf": [
                                {
                                    "type": "string"
                                },
                                {
                                    "type": "null"
                                }
                            ]
                        },
                        "port": {
                            "oneOf": [
                                {
                                    "type": "number"
                                },
                                {
                                    "type": "null"
                                }
                            ]
                        },
                        "user": {
                            "oneOf": [
                                {
                                    "type": "string"
                                },
                                {
                                    "type": "null"
                                }
                            ]
                        },
                        "password": {
                            "oneOf": [
                                {
                                    "type": "string"
                                },
                                {
                                    "type": "null"
                                }
                            ]
                        },
                        "password_file": {
                            "oneOf": [
                                {
                                    "type": "string"
                                },
                                {
                                    "type": "null"
                                }
                            ]
                        },
                        "db_name": {
                            "oneOf": [
                                {
                                    "type": "string"
                                },
                                {
                                    "type": "null"
                                }
                            ]
                        },
                        "ssl": {
                            "oneOf": [
                                {
                                    "type": "boolean"
                                },
                                {
                                    "type": "null"
                                }
                            ]
                        }
                    },
                    "required": []
                },
                {
                    "type": "null"
                }
            ]
        }
    },
    "required": []
}
